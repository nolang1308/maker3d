{
  "name": "maker3d-backend",
  "version": "1.0.0",
  "description": "Backend server for 3D printing store",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node_modules/.bin/nodemon server.js",
    "tunnel": "cloudflared.exe tunnel --config config.yml run maker3d-backend",
    "dev:all": "concurrently \"npm run dev\" \"npm run tunnel\""
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
